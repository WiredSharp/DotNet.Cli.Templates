{
    "$schema": "http://json.schemastore.org/template",
    "author": "WiredSharp",
    "classifications": ["Project"],
    "identity": "WiredSharp.DirectoryBuildProps",
    "name": "WiredSharp Directory Build Props",
    "shortName": "wiredbuildprops",
    "sourceName": "z-project",
    "preferNameDirectory": true,
    "tags": {
        "type": "solution"
    },
    "symbols": {
        "Author": {
            "type": "parameter",
            "defaultValue": "you",
            "replaces": "$Author"
        },
        "Company": {
            "type": "parameter",
            "defaultValue": "your company",
            "replaces": "$Company"
        },
        "ArtifactFolder": {
            "type": "parameter",
            "defaultValue": "artifacts",
            "replaces": "$ArtifactFolder"
        },
        "IntermediateFolder": {
            "type": "parameter",
            "defaultValue": "artifacts/obj",
            "replaces": "$IntermediateFolder"
        }
    }
}