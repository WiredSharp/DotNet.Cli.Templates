{
    "$schema": "http://json.schemastore.org/template",
    "author": "zericco",
    "classifications": ["Solution"],
    "identity": "Zericco.SolutionPackTemplate.CSharp",
    "name": "Zericco Solution pack",
    "shortName": "zericcosol",
    "sourceName": "z-project",
    "tags": {
        "language": "C#",
        "type": "project"
    },
    "symbols": {
        "ArtifactFolder": {
            "type": "parameter",
            "defaultValue": "artifacts",
            "replaces": "${ArtifactFolder}"
        },
        "IntermediateFolderName": {
            "type": "parameter",
            "defaultValue": "artifacts/obj",
            "replaces": "${IntermediateFolder}"
        }
    }
}